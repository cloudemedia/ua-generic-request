{
    "name": "mimmi20/wurfl-generic-request",
    "description": "the Generic Request Object extracted from Wurfl for PHP 5.3",
    "type": "library",
    "license": "LGPL-2.1",
    "keywords": ["Wurfl", "Browser", "user agent", "http", "parser", "user-agent"],
    "homepage": "https://github.com/mimmi20/wurfl-generic-request",
    "require": {
        "php": ">=5.3.3",
        "mimmi20/wurfl-constants": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Wurfl\\Request\\": "src/"
        }
